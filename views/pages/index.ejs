<!DOCTYPE html>
<html lang="en">

<head>

  <%- include('../partials/head') %>
</head>

<body class="container">
  <header>

    <%- include('../partials/header') %>
  </header>
  <main>
    <div class="jumbotron">

      <h1>EJS view engine!</h1>
    </div>
    <h2>Bebidas</h2>
    <table class="table table-bordered">



      <tbody>
        <% bebidas.forEach(function(bebida) { %>
        <tr>

          <td><%= bebida.nome %></td>

          <td><%= bebida.total %></td>

        </tr>
        <% }); %>

      </tbody>
    </table>
  </main>
  <footer>

    <%- include('../partials/footer') %>
  </footer>